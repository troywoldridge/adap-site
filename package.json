{
  "name": "adap-site",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "engines": {
    "node": ">=18 <=22"
  },
  "scripts": {
    "db:introspect": "drizzle-kit introspect",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed:subcategories": "pnpm exec ts-node --esm scripts/seed-subcategories.ts",
    "generate:migrations": "pnpm drizzle-kit generate",
    "push:schema": "pnpm drizzle-kit push",
    "generate:types": "pnpm drizzle-kit generate:types",
    "build:hashes": "pnpm tsx scripts/buildAllOptionHashes.ts",
    "watch:hashes": "nodemon --watch drizzle/migrations/schema.ts --exec \"pnpm build:hashes\"",
    "sync:sinalite": "node scripts/syncSinalite.js",
    "export:hero-analytics": "node scripts/parse-hero-analytics.js",
    "deploy:full": "bash scripts/deploy.sh develop origin"
  },
  "dependencies": {
    "@vercel/og": "^0.8.5",
    "algoliasearch": "^4.14.2",
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.3",
    "instantsearch.css": "^8.5.1",
    "lucide-react": "^0.536.0",
    "next": "14.2.3",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.5.0",
    "react-instantsearch": "^7",
    "react-instantsearch-hooks-web": "^6.47.3",
    "satori": "^0.16.2",
    "sharp": "^0.34.3",
    "slugify": "^1.6.6",
    "swr": "^2.3.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20.19.9",
    "@types/pg": "^8.15.4",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
